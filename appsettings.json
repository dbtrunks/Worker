{
		"NLog": {
			"throwConfigExceptions": true,
			"targets": {
				"async": true,
				"logfile": {
					"type": "File",
					"fileName": "./logs/log_${shortdate}.log"
				},
				"logconsole": {
					"type": "ColoredConsole"
				}
			},
			"rules": [
				{
					"logger": "*",
					"minLevel": "Info",
					"writeTo": "logconsole"
				},
				{
					"logger": "*",
					"minLevel": "Debug",
					"writeTo": "logfile"
				}
			]
		},

"DoInterval": 12
}
